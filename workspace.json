{
  "version": 2,
  "projects": {
    "js-nawah": {
      "root": "libs/js-nawah",
      "sourceRoot": "libs/js-nawah/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/js-nawah/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/js-nawah"],
          "options": {
            "jestConfig": "libs/js-nawah/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  }
}
